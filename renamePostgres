/var/lib/postgresql/data/
postgresql.conf
wal_level = logical                     # minimal, replica, or logical
wal_level = replica


docker exec microserv-postgres-1 sed -i 's/^#wal_level = replica/wal_level = logical/' /var/lib/postgresql/data/postgresql.conf
